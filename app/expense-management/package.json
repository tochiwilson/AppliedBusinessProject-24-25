{
  "name": "expensemanagement",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "deploy-config_old": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#expensemanagement-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandboxMockServer.html?sap-ui-xx-viewCache=false#expensemanagement-display\"",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "build": "ui5 build -a --clean-dest --include-task=generateCachebusterInfo",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#expensemanagement-display\""
  },
  "devDependencies": {
    "@sap/ui5-builder-webide-extension": "^1.1.8",
    "ui5-task-zipper": "^0.5.0",
    "mbt": "^1.2.18",
    "@ui5/cli": "^3.0.0",
    "@sap/ux-ui5-tooling": "1",
    "rimraf": "3.0.2",
    "@sap-ux/ui5-middleware-fe-mockserver": "2"
  },
  "ui5": {
    "dependencies": [
      "@sap/ui5-builder-webide-extension",
      "ui5-task-zipper",
      "mbt"
    ]
  },
  "private": true,
  "sapuxLayer": "CUSTOMER_BASE"
}